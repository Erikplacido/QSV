{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    }
  ],
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node",
      "includeFiles": "backend/**"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
